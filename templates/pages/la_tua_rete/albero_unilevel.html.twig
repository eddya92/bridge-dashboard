{% block javascript_page %}
    <script>

		var utenteConnesso = "{{ app.user.nominativo }}";

		function salva_nuova_vista(){
			$('#idVista').val(0);
			document.form.submit();
		}

		//region zoom sulla vsita dell'albero
		function zoomVistaAlbero(){
			let elem = document.getElementById('row_tree_filters');

			if(!document.fullscreenElement){
				elem.requestFullscreen().catch(err => {
					alert(`Error attempting to enable full-screen mode: ${err.message} (${err.name})`);
				});
			} else{
				document.exitFullscreen();
			}
		}
		//endregion

		function elimina_vista(){
			$('[name="idVistaElimina"]').val($('[name="idVista"]').val());
			document.form_vista_elimina.submit();
		}

		function print_tree(){
			window.print();
		}

		$(document).ready(function(){
			$('#type-sottoposti').typeahead(
				{
					hint: true,
					highlight: true,
					minLength: 3
				},
				{
					limit: 50,
					async: true,
					name: 'result',
					displayKey: 'displayKey',
					source: function(query, processSync, processAsync){
						//processSync(['This suggestion appears immediately', 'This one too']);
						return $.ajax({
							url: '{{ path('utenti-ajax') }}',
							type: 'GET',
							data: {query: query},
							dataType: 'json',
							success: function(json){
								var elems = [];
								$(json).each(function(i, j){
									j['displayKey'] = j.nominativo;
									elems.push(j);
								});

								return processAsync(elems);
							}
						});
					}
				}
			).bind('typeahead:select', function(evt, suggestion){
				$('[name="idUtente"]').val(suggestion.id);
				aggiorna_albero();
			});

			aggiorna_albero();
		});

		//--------------------------------------------------------------------------------------------------------------
		var livello = 0;
		var orientamento = 'verticale';
		var tree;
		var root;
		var vis;
		var diagonal;
		var tooltip;

		function aggiorna_albero(){
			$('#body_albero').empty();
			$('.tooltip-albero').remove();

			var idVista = $('[name="idVista"]').val();
			if(idVista == ''){
				idVista = 0;
			}

			var idUtente = $('[name="idUtente"]').val();
			if(idUtente == ''){
				idUtente = 0;
			}

			livello = $('[name="livello"]').val();
			var mese = $('[name="mese"]').val();

			var punti = $('[name="punti"]').val();
			if(punti == ''){
				punti = 'pv_mensile';
			}

			orientamento = $('[name="orientamento"]').val();
			if(orientamento != 'orizzontale' && orientamento != 'verticale'){
				orientamento = 'verticale';
			}

			var width = $('[name="larghezza"]').val();
			if(width == 'auto'){
				width = 900;
			}

			var height = $('[name="altezza"]').val();
			if(height == 'auto'){
				height = 900;
			}

			// --------------------

			var hsvg = $('#body_albero').height() - 0;

			var wsvg = 10000;
			if(orientamento == 'verticale'){
				var wsvg = $('#body_albero').width() - 20;
			}

			var m = [20, 170, 20, 170];
			var w = wsvg - m[1] - m[3];
			if(width > 0){
				w = width - m[1] - m[3];
			}
			var h = hsvg - m[0] - m[2];
			if(height > 0){
				h = height - m[0] - m[2];
			}

			tree = d3.layout.tree().size([h, w]);
			diagonal = d3.svg.diagonal().projection(function(d){
				return [d.y, d.x];
			});

			if(orientamento == 'verticale'){
				tree = d3.layout.tree().size([w, h]);
				diagonal = d3.svg.diagonal().projection(function(d){
					return [d.x, d.y];
				});
			}

			vis = d3.select('#body_albero').append('svg:svg')
				.attr('width', w + m[1] + m[3])
				.attr('height', h + m[0] + m[2])
				.append('svg:g')
				.attr('transform', 'translate(' + m[3] + "," + m[0] + ')');

			tooltip = d3.select('body')
				.append('div')
				.style('position', 'absolute')
				.style('z-index', '10')
				.style('height', '130px')
				.style('visibility', 'hidden')
				.attr('class', 'tooltip-albero')
				.text('');

			var url = '{{ path('albero-unilevel-ajax') }}?idUtente=' + idUtente + '&mese=' + mese + '&livello=' + livello + '&punti=' + punti + '&idVista=' + idVista;
			d3.json(url, function(json){
				root = json;
				if(root == 'error'){
					alert('Si è verificato un problema con la generazione dell\'albero.');
				} else{
					root.x0 = h / 2;
					if(orientamento == 'verticale'){
						root.y0 = w / 2;
					} else{
						root.y0 = 10;
					}

					if(utenteConnesso != root.name){
						$('#type-sottoposti').val(root.name)
					}

					root.children.forEach(toggleAll);
					root.children.forEach(toggleAllLevels);
					update(root);
				}
			});
		}

		function populateTooltip(d){
			return d.tooltip;
		}

		function update(source){
			var i = 0;

			var duration = d3.event && d3.event.altKey ? 5000 : 500;

			var nodes = tree.nodes(root).reverse();
			if(orientamento == 'verticale'){
				nodes.forEach(function(d){
					d.y = d.depth * 100;
				});
			} else{
				nodes.forEach(function(d){
					d.y = d.depth * 180;
				});
			}

			var node = vis.selectAll('g.node').data(nodes, function(d){
				return d.id || (d.id = ++i);
			});

			if(orientamento == 'verticale'){
				var nodeEnter = node.enter().append('svg:g')
					.attr('class', 'node')
					.attr('transform', function(d){
						return 'translate(' + source.x0 + ',' + source.y0 + ')';
					})
					.on('click', function(d){
						toggle(d);
						update(d);
					})
					.on('contextmenu', function(d){
						d3.event.preventDefault();
					});
			} else{
				var nodeEnter = node.enter().append('svg:g')
					.attr('class', 'node')
					.attr('transform', function(d){
						return 'translate(' + source.y0 + ',' + source.x0 + ')';
					})
					.on('click', function(d){
						toggle(d);
						update(d);
					})
					.on('contextmenu', function(d){
						d3.event.preventDefault();
					});
			}

			nodeEnter.append('svg:image')
				.attr('x', -9)
				.attr('y', -12)
				.attr('width', 20)
				.attr('height', 24)
				.attr("xlink:href", function(d){
					return "../assets/icone_albero/Promoter-Leader.svg";
				})
				.attr('r', 1e-6)
				.on('mouseover', function(d){
					return tooltip.style('visibility', 'visible').html(populateTooltip(d));
				})
				.on('mousemove', function(){
					return tooltip.style('top', (d3.event.pageY - 10) + 'px').style('left', (d3.event.pageX + 10) + 'px');
				})
				.on('mouseout', function(){
					return tooltip.style('visibility', 'hidden');
				})
				.style('fill', function(d){
					return d._children ? 'lightsteelblue' : "#fff";
				})

			if(orientamento == 'verticale'){
				nodeEnter.append('svg:text')
					.attr("y", function(d){
						return d.children || d._children ? 14 : 14;
					})
					.attr('dy', '.35em')
					.attr('text-anchor', function(d){
						return d.children || d._children ? 'start' : 'start';
					})
					.text(function(d){
						var txt = '';
						txt = txt + d.name;
						if(d.punti != 'assenti'){
							txt = txt + ' [' + d.punti + ']';
						}
						return txt;
					})
					.style('fill-opacity', 1e-6)
					.style('font-style', 'normal')
					.style('fill', function(d){
						return d.children || d._children ? '000000' : 'blue';
					});

				nodeEnter.append('svg:image')
					.attr('width', '19px')
					.attr('height', '19px')
					.attr('text-anchor', function(d){
						return d.children || d._children ? 'end' : 'start';
					})
					.attr('y', '15px')
					.style('opacity', function(d){
						if(d.icona_guida === '1'){
							if(d.Qualificato === '1'){
								return 1;
							} else{
								return 0.2;
							}
						} else{
							return 0;
						}
					});

				var nodeUpdate = node.transition()
					.duration(duration)
					.attr('transform', function(d){
						return 'translate(' + d.x + ',' + d.y + ')';
					});
			} else{
				nodeEnter.append('svg:text')
					.attr('x', function(d){
						return d.children || d._children ? -10 : 10;
					})
					.attr('dy', '.35em')
					.attr('text-anchor', function(d){
						return d.children || d._children ? 'end' : 'start';
					})
					.text(function(d){
						var txt = '';
						txt = txt + d.name;
						if(d.punti != 'assenti'){
							txt = txt + ' [' + d.punti + ']';
						}
						return txt;
					})
					.style('fill-opacity', 1e-6)
					.style('font-style', 'normal')
					.style('fill', function(d){
						return d.children || d._children ? '000000' : 'blue';
					});

				nodeEnter.append('svg:image')
					.attr('width', '19px')
					.attr('height', '19px')
					.attr('text-anchor', function(d){
						return d.children || d._children ? 'start' : 'start';
					})
					.attr('y', '15px')
					.style('opacity', function(d){
						if(d.icona_guida === '1'){
							if(d.Qualificato === '1'){
								return 1;
							} else{
								return 0.2;
							}
						} else{
							return 0;
						}
					});

				var nodeUpdate = node.transition()
					.duration(duration)
					.attr('transform', function(d){
						return 'translate(' + d.y + ',' + d.x + ')';
					});
			}

			nodeUpdate.select('circle')
				.attr('r', 4.5)
				.style('fill', function(d){
					return d._children ? 'lightsteelblue' : '#fff';
				});

			nodeUpdate.select('image').style('opacity', function(d){
				if(d.Qualificato === '1'){
					return 1;
				} else{
					return 0.2;
				}
			});

			nodeUpdate.select("text")
				.style("fill-opacity", 1);

			if(orientamento == 'verticale'){
				var nodeExit = node.exit().transition()
					.duration(duration)
					.attr('transform', function(d){
						return 'translate(' + source.x + ',' + source.y + ')';
					})
					.remove();
			} else{
				var nodeExit = node.exit().transition()
					.duration(duration)
					.attr('transform', function(d){
						return 'translate(' + source.y + ',' + source.x + ')';
					})
					.remove();
			}

			nodeExit.select('circle')
				.attr('r', 1e-6);

			nodeExit.select('text')
				.style('fill-opacity', 1e-6);

			var link = vis.selectAll('path.link')
				.data(tree.links(nodes), function(d){
					return d.target.id;
				});

			link.enter().insert('svg:path', 'g')
				.attr('class', 'link')
				.attr('d', function(d){
					var o = {x: source.x0, y: source.y0};
					return diagonal({source: o, target: o});
				})
				.transition()
				.duration(duration)
				.attr('d', diagonal);

			link.transition()
				.duration(duration)
				.attr('d', diagonal);

			link.exit().transition()
				.duration(duration)
				.attr('d', function(d){
					var o = {x: source.x, y: source.y};
					return diagonal({source: o, target: o});
				})
				.remove();

			nodes.forEach(function(d){
				d.x0 = d.x;
				d.y0 = d.y;
			});
		}

		function toggle(d){
			if(d.children){
				d._children = d.children;
				d.children = null;
			} else{
				d.children = d._children;
				d._children = null;
			}
		}

		function toggleAll(d){
			if(d.children){
				d.children.forEach(toggleAll);
				toggle(d);
			}
		}

		function toggleAllLevels(d){
			if(d.livello < livello){
				toggle(d);
				if(d.children){
					d.children.forEach(toggleAllLevels);
				}
			}
		}

    </script>

